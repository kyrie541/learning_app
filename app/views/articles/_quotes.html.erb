<div class="tab-pane fade overflow" id="quote" role="tabpanel" aria-labelledby="quote-tab">
<div class="row" style="padding-top:5%;">
<div class="col"></div>
<div class="col-md-7">

<h1>Quote Generater</h1>

<div id="quote1">Quote Goes Here...</div>
<br>
<button id="quotebutton" class="btn btn-success">Click Me!</button>


</div> <!-- .col-md-7 -->
<div class="col"></div>
</div> <!-- .row -->
</div>

<script>
  var url = 'https://quotesondesign.com/wp-json/posts?filter[orderby]=rand&filter[posts_per_page]=1&callback='
  $('#quotebutton').click(function(){
    $.getJSON(url)
    .done(function(data){
      $('#quote1').html(data[0]["content"]);
    });
  });

</script>
